{
  "name": "agicash",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max-old-space-size=8192' tsx watch ./app/dev-server.ts",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome lint --write",
    "lint:check": "biome lint",
    "format": "biome format --write",
    "format:check": "biome format",
    "fix:all": "biome check --write --verbose",
    "fix:staged": "biome check --write --staged --verbose",
    "check:all": "run-p typecheck lint:check format:check",
    "test": "bun test",
    "test:e2e": "playwright test",
    "supabase": "supabase",
    "db:generate-types": "supabase gen types typescript --local --schema wallet > supabase/database.types.ts"
  },
  "scripts:comments": {
    "dev": "We are using tsx instead of bun because bun doesn't support all node APIs that development server needs.",
    "start": "We are running the build in node because atm we bun doesn't support everything needed yet."
  },
